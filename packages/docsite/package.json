{
  "name": "react-dnd-documentation",
  "description": "React DnD Docsite",
  "version": "11.1.3",
  "private": true,
  "license": "MIT",
  "dependencies": {
    "debug": "^4.2.0",
    "gatsby": "~2.24.80",
    "gatsby-plugin-manifest": "^2.4.35",
    "gatsby-plugin-react-helmet": "^3.3.14",
    "gatsby-plugin-sharp": "^2.6.42",
    "gatsby-plugin-styled-components": "^3.3.14",
    "gatsby-plugin-typescript": "^2.4.22",
    "gatsby-plugin-typography": "^2.5.13",
    "gatsby-remark-autolink-headers": "^2.3.15",
    "gatsby-remark-copy-linked-files": "^2.3.19",
    "gatsby-remark-images": "^3.3.35",
    "gatsby-remark-prismjs": "^3.5.16",
    "gatsby-source-filesystem": "^2.3.35",
    "gatsby-transformer-remark": "^2.8.42",
    "gatsby-transformer-sharp": "^2.5.18",
    "lodash": "^4.17.20",
    "prismjs": "^1.22.0",
    "query-string": "^6.13.5",
    "react": "^16.14.0",
    "react-dnd": "workspace:packages/react-dnd",
    "react-dnd-examples-decorators": "workspace:packages/examples-decorators",
    "react-dnd-examples-hooks": "workspace:packages/examples-hooks",
    "react-dnd-html5-backend": "workspace:packages/backend-html5",
    "react-dnd-test-backend": "workspace:packages/backend-test",
    "react-dnd-touch-backend": "workspace:packages/backend-touch",
    "react-dom": "^16.14.0",
    "react-error-overlay": "^6.0.7",
    "react-helmet": "^6.1.0",
    "react-is": "^16.13.1",
    "react-tabs": "^3.1.1",
    "react-typography": "^0.16.19",
    "rehype-react": "^6.1.0",
    "shallow-compare": "^1.2.2",
    "styled-components": "^5.2.0",
    "typography": "^0.16.19",
    "typography-theme-github": "^0.16.19"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/query-string": "^6.3.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.1.0",
    "@types/react-tabs": "^2.3.2",
    "@types/rehype-react": "^4.0.0",
    "@types/styled-components": "^5.1.4",
    "babel-plugin-styled-components": "^1.11.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "prop-types": "^15.7.2",
    "rimraf": "^3.0.2",
    "shx": "^0.3.2"
  },
  "scripts": {
    "clean": "rimraf .cache public apidocs static/examples_decorators_js static/examples_decorators_ts static/examples_hooks_js static/examples_hooks_ts",
    "copy_examples:js_hooks": "shx cp -r ../examples-hooks/dist/docs static/examples_hooks_js",
    "copy_examples:ts_hooks": "shx cp -r ../examples-hooks/src static/examples_hooks_ts",
    "copy_examples:js_decorators": "shx cp -r ../examples-decorators/dist/docs static/examples_decorators_js",
    "copy_examples:ts_decorators": "shx cp -r ../examples-decorators/src static/examples_decorators_ts",
    "copy_examples": "run-s 'copy_examples:*'",
    "prune_buildinfo": "rimraf static/examples_hooks_js/tsconfig.docs.tsbuildinfo static/examples_decorators_js/tsconfig.docs.tsbuildinfo",
    "format_examples": "prettier 'static/examples_*/**/*.(js|ts|tsx)' --use-tabs=false --write",
    "write_example_boilerplate": "node write_sample_boilerplate.js",
    "process_examples": "run-s copy_examples prune_buildinfo format_examples write_example_boilerplate",
    "build:gatsby": "gatsby build --prefix-paths",
    "start:docsite": "gatsby develop -H 0.0.0.0",
    "build:docsite": "run-s process_examples build:gatsby"
  }
}
