{
	"name": "react-dnd-html5-backend",
	"version": "5.0.1",
	"description": "HTML5 backend for React DnD",
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"license": "BSD-3-Clause",
	"repository": {
		"type": "git",
		"url": "https://github.com/react-dnd/react-dnd.git"
	},
	"scripts": {
		"clean": "rimraf lib dist",
		"transpile": "tsc",
		"bundle:unmin": "webpack --mode development --output-filename=ReactDnDHTML5Backend.js",
		"bundle:min": "webpack --mode production --output-filename=ReactDnDHTML5Backend.min.js",
		"build": "run-p bundle:* transpile",
		"test": "run-s clean build",
		"watch": "tsc -w --preserveWatchOutput",
		"start": "npm run watch"
	},
	"dependencies": {
		"autobind-decorator": "^2.1.0",
		"dnd-core": "^4.0.5",
		"lodash": "^4.17.11",
		"shallowequal": "^1.1.0"
	},
	"devDependencies": {
		"@types/react": "^16.3.16",
		"npm-run-all": "^4.1.3",
		"react": "^16.4.0",
		"react-dnd-test-backend": "^5.0.1",
		"rimraf": "^2.6.2",
		"ts-loader": "^5.2.2",
		"typescript": "^2.9.1",
		"webpack": "^4.23.1"
	}
}
