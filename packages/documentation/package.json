{
	"name": "react-dnd-documentation",
	"description": "React DnD Docsite",
	"version": "7.6.2",
	"private": true,
	"license": "MIT",
	"dependencies": {
		"@types/query-string": "^6.2.0",
		"@types/react": "^16.8.19",
		"@types/react-dom": "^16.8.4",
		"@types/react-helmet": "^5.0.8",
		"@types/react-tabs": "^2.3.1",
		"gatsby": "^2.1.23",
		"gatsby-image": "^2.0.31",
		"gatsby-plugin-manifest": "^2.0.22",
		"gatsby-plugin-offline": "^2.0.24",
		"gatsby-plugin-react-helmet": "^3.0.8",
		"gatsby-plugin-sharp": "^2.0.25",
		"gatsby-plugin-styled-components": "^3.0.7",
		"gatsby-plugin-typescript": "^2.0.10",
		"gatsby-plugin-typography": "^2.2.8",
		"gatsby-remark-autolink-headers": "^2.0.16",
		"gatsby-remark-copy-linked-files": "^2.0.9",
		"gatsby-remark-embed-snippet": "^3.2.2",
		"gatsby-remark-images": "^3.0.6",
		"gatsby-remark-prismjs": "^3.2.4",
		"gatsby-source-filesystem": "^2.0.23",
		"gatsby-transformer-remark": "^2.3.1",
		"gatsby-transformer-sharp": "^2.1.15",
		"prismjs": "^1.15.0",
		"query-string": "^6.5.0",
		"react": "^16.8.6",
		"react-dnd": "^7.6.2",
		"react-dnd-examples-decorators": "^7.6.2",
		"react-dnd-examples-hooks": "^7.6.2",
		"react-dnd-html5-backend": "^7.6.2",
		"react-dnd-test-backend": "^7.6.2",
		"react-dom": "^16.8.6",
		"react-helmet": "^5.2.0",
		"react-tabs": "^3.0.0",
		"react-typography": "^0.16.18",
		"rehype-react": "^3.1.0",
		"styled-components": "^4.2.0",
		"typography": "^0.16.18",
		"typography-theme-github": "^0.16.18"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"devDependencies": {
		"gh-pages": "^2.0.1",
		"npm-run-all": "^4.1.5",
		"prettier": "^1.18.0"
	},
	"scripts": {
		"clean": "rimraf .cache public apidocs static/examples_decorators_js static/examples_decorators_ts static/examples_hooks_js static/examples_hooks_ts",
		"start": "gatsby develop",
		"copy_examples:js_hooks": "cp -r ../examples-hooks/lib/docs static/examples_hooks_js",
		"copy_examples:ts_hooks": "cp -r ../examples-hooks/src static/examples_hooks_ts",
		"copy_examples:js_decorators": "cp -r ../examples-decorators/lib/docs static/examples_decorators_js",
		"copy_examples:ts_decorators": "cp -r ../examples-decorators/src static/examples_decorators_ts",
		"copy_examples": "run-s copy_examples:*",
		"prune_buildinfo": "rimraf static/examples_hooks_js/tsconfig.docs.tsbuildinfo static/examples_decorators_js/tsconfig.docs.tsbuildinfo",
		"format_examples": "prettier 'static/examples_*/**/*' --use-tabs=false --write",
		"write_example_boilerplate": "node write_sample_boilerplate.js",
		"process_examples": "run-s copy_examples prune_buildinfo format_examples write_example_boilerplate",
		"build_static_site": "gatsby build --prefix-paths",
		"publish_gh_pages": "NODE_DEBUG=gh-pages gh-pages -d public -t",
		"release": "run-s clean process_examples build_static_site publish_gh_pages"
	}
}
